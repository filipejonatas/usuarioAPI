generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum role{
  Admin
  User
  Manager
}



model User {
  Id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  role      String?
  password  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}